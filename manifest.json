{
  "name": "Fast Jump",
  "description": "allow customize your hotkeys",
  "permissions": ["declarativeContent", "contextMenus", "activeTab", "storage", "tabs"],
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentScript.js"]
  }],
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/fastjump16.png",
      "48": "images/fastjump48.png",
      "128": "images/fastjump128.png"
    }
  },
  "icons": {
    "16": "images/fastjump16.png",
    "48": "images/fastjump48.png",
    "128": "images/fastjump128.png"
  }
}